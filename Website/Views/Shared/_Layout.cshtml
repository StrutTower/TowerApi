<!DOCTYPE html>
<html>
<partial name="_LayoutHead" />
<body>
    <partial name="_LayoutNav" />

    <noscript class="noscript">
        JavaScript is disabled in your browser.
        <br />
        Most of this site will not function properly without JavaScript enabled.
    </noscript>

    <main class="container">
        @RenderBody()
    </main>

    <partial name="_LayoutFooter" />

    <environment include="Development">
        <script src="~/lib/lib.js" asp-append-version="true"></script>
        <script asp-src-include="~/js/*" asp-append-version="true"></script>
    </environment>
    <environment exclude="Development">
        <script src="~/lib/bundle.min.js" asp-append-version="true"></script>
    </environment>
    @RenderSection("Scripts", required: false)
</body>
</html>
